<%@ page contentType="text/html;charset=UTF-8" language="java" %>
<html>
<head>
    <title>showCustomer</title>
    <style >

        a {
            text-decoration:none;
        }
    </style>
</head>
<body>
<div class="rt_content">
    <div class="page_title">
        <h2 class="fl">用户列表展示</h2>
        <a href="<%=request.getContextPath()%>/customer/add" class="fr top_rt_btn add_icon">添加用户</a>
    </div>
    <section class="mtb">
        <select class="select">
            <option>选择性别</option>
            <option>女</option>
            <option>男</option>
        </select>
        <input type="text" class="textbox textbox_225" placeholder="输入用户姓名"/>
        <input type="button" value="查询" class="group_btn"/>
    </section>
    <table class="table">
        <tr>
            <th>用户编号</th>
            <th>用户姓名</th>
            <th>用户电话</th>
            <th>用户地址</th>
            <th>用户性别</th>
            <th>操作</th>
        </tr>
        <tr>
            <td>用户编号</td>
            <td>用户姓名</td>
            <td>用户电话</td>
            <td>用户地址</td>
            <td>用户性别</td>
            <td>
                <a href="" title="编辑" class="link_icon">编辑</a>
                <a href="" title="删除" class="link_icon">删除</a>
            </td>
        </tr>
    </table>
    <aside class="paging">
        <a>第一页</a>
        <a>1</a>
        <a>2</a>
        <a>3</a>
        <a>…</a>
        <a>1004</a>
        <a>最后一页</a>
    </aside>
</div>
</body>
</html>
