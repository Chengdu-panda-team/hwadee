<%@ page contentType="text/html;charset=UTF-8" language="java" %>
<html>
<head>
    <title>新增用户信息</title>
    <link rel="stylesheet" type="text/css" href="<%=request.getContextPath()%>/resources/css/bootstrap.css">
    <style>
        body {
            background:url("<%=request.getContextPath() %>/resources/images/background2.png");
        }
    </style>
</head>
<body>
    <div align="center">
        <h4>新增用户信息</h4>
        <form action="">
            <table width="500px" height="300px">
                <tr>
                    <td style="width:120px;">用户名称</td><td><input type="text" name="customerName"></td>
                </tr>
                <tr>
                    <td style="width:120px;">性别</td>
                    <td>
                        <input type="radio" name="customerSex" id="nan"> <label for="nan">男</label>
                        <input type="radio" name="customerSex" id="nv"> <label for="nv">女</label>
                    </td>
                </tr>
                <tr>
                    <td style="width:120px;">电话</td><td><input type="tel" name="customerTel"></td>
                </tr>
                <tr>
                    <td style="width:120px;">地址</td><td><input type="text" name="customerAddress"></td>
                </tr>
                <tr>
                    <td></td>
                    <td>
                    <input type="submit" value="提交">
                    <input type="button" value="取消" onclick="history.go(-1)">
                    </td>
                </tr>
            </table>
        </form>
    </div>
</body>
</html>
